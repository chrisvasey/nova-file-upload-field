{
    "name": "chrisvasey/nova-file-upload-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "illuminate/http": "^5.8|^6.0",
        "illuminate/support": "^5.8|^6.0",
        "laravel/nova": "*",
        "rodneyrehm/plist": "*",
        "symfony/thanks": "*"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\NovaFileUploadField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\NovaFileUploadField\\Providers\\Service"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
